datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Lead {
  id                  String   @id @default(auto()) @map("_id") @db.ObjectId
  name                String
  contactInfo         String
  conversionProbability Float
  createdAt           DateTime @default(now())
}